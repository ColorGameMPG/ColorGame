{
    "tutorial_receiver": [
        {
            "delay": 2.5,
            "selector": ".receiverSymbolView",
            "phrase": "tutorial_bubble_sender_icon"
        },
        {
            "selector": ".receiverColorView",
            "phrase": "tutorial_bubble_sender_splash"
        }
    ],

    "tutorial_sender": [
        {
            "delay": 2.5,
            "selector": ".selected",
            "phrase": "tutorial_bubble_receiver_splash"
        },
        {
            "selector": ".senderColorView",
            "phrase": "tutorial_bubble_receiver_colors"
        },
        {
            "selector": ".senderSymbolView",
            "phrase": "tutorial_bubble_receiver_icon"
        }
    ],

    "tutorial_sender_first_symbol": [
        {
            "delay": 1.0,
            "selector": ".senderSymbol",
            "phrase": "tutorial_bubble_delete"
        },
        {
            "selector": ".confirmTrial",
            "phrase": "tutorial_bubble_send"
        }
