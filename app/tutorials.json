        {
            "selector": ".newPuzzle",
            "phrase": "tutorial_bubble_new"
        }
    ],

    "lobby_online": [
        {
            "delay": 1.0,
            "selector": ".status .online:visible, .status .playing:visible",
            "phrase": "tutorial_bubble_status"
        }
    ],

    "lobby_stress": [
        {
            "delay": 1.0,
            "selector": ".newStressPuzzle",
            "phrase": "tutorial_bubble_stress"
