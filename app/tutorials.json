    "lobby": [
        {
            "delay": 7.0,
            "selector": ".rank",
            "phrase": "tutorial_bubble_rank",
            "holePadding": 0
        },
        {
            "selector": ".lobbyHeader",
            "phrase": "tutorial_bubble_score"
        },
        {
            "selector": ".senderPuzzleCount",
            "phrase": "tutorial_bubble_puzzles"
